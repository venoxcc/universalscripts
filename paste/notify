function createBaseNotifications()if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("NotificationHolder")then return game:GetService("Players").LocalPlayer.PlayerGui.NotificationHolder end;local a=Instance.new("ScreenGui")a.ZIndexBehavior=Enum.ZIndexBehavior.Sibling;local b=Instance.new("Frame")b.Name="ToggleNotif"b.ZIndex=5;b.AnchorPoint=Vector2.new(1,1)b.Visible=false;b.Size=UDim2.new(0,291,0,56)b.Position=UDim2.new(1,0,1,0)b.BackgroundColor3=Color3.fromRGB(48,48,48)b.Parent=a;local c=Instance.new("UICorner")c.Name="UiCorner"c.Parent=b;local d=Instance.new("UIStroke")d.Name="Dropshadow"d.ApplyStrokeMode=Enum.ApplyStrokeMode.Border;d.Transparency=0.8;d.Thickness=2;d.Color=Color3.fromRGB(20,20,20)d.Parent=b;local e=Instance.new("Frame")e.Name="SepVertical"e.Size=UDim2.new(0,2,0,56)e.BackgroundTransparency=0.5;e.Position=UDim2.new(0.7423077,0,0,0)e.BorderSizePixel=0;e.BackgroundColor3=Color3.fromRGB(68,68,68)e.Parent=b;local f=Instance.new("Frame")f.Name="SepHorizontal"f.Size=UDim2.new(0,72,0,2)f.BackgroundTransparency=0.5;f.Position=UDim2.new(0.75,0,0.4464286,2)f.BorderSizePixel=0;f.BackgroundColor3=Color3.fromRGB(68,68,68)f.Parent=b;local g=Instance.new("TextLabel")g.Name="Title"g.Size=UDim2.new(0,216,0,19)g.BackgroundTransparency=1;g.BorderSizePixel=0;g.BackgroundColor3=Color3.fromRGB(255,255,255)g.FontSize=Enum.FontSize.Size14;g.TextSize=14;g.TextColor3=Color3.fromRGB(255,255,255)g.Font=Enum.Font.SourceSans;g.Parent=b;local h=Instance.new("TextLabel")h.Name="Paragraph"h.Size=UDim2.new(0,218,0,37)h.BackgroundTransparency=1;h.Position=UDim2.new(0,0,0.3392857,0)h.BackgroundColor3=Color3.fromRGB(255,255,255)h.FontSize=Enum.FontSize.Size14;h.TextSize=14;h.TextColor3=Color3.fromRGB(255,255,255)h.Text=""h.TextYAlignment=Enum.TextYAlignment.Top;h.TextWrapped=true;h.Font=Enum.Font.SourceSans;h.TextWrap=true;h.TextXAlignment=Enum.TextXAlignment.Left;h.Parent=b;local i=Instance.new("UIPadding")i.PaddingLeft=UDim.new(0,10)i.PaddingRight=UDim.new(0,5)i.Parent=h;local j=Instance.new("TextButton")j.Name="True"j.Size=UDim2.new(0,72,0,27)j.BackgroundTransparency=1;j.Position=UDim2.new(0.75,0,0,0)j.BorderSizePixel=0;j.BackgroundColor3=Color3.fromRGB(255,255,255)j.FontSize=Enum.FontSize.Size14;j.TextSize=14;j.TextColor3=Color3.fromRGB(255,255,255)j.Text="Yes"j.Font=Enum.Font.SourceSans;j.Parent=b;local k=Instance.new("TextButton")k.Name="False"k.Size=UDim2.new(0,72,0,27)k.BackgroundTransparency=1;k.Position=UDim2.new(0.75,0,0.5178571,0)k.BorderSizePixel=0;k.BackgroundColor3=Color3.fromRGB(255,255,255)k.FontSize=Enum.FontSize.Size14;k.TextSize=14;k.TextColor3=Color3.fromRGB(255,255,255)k.Text="No"k.Font=Enum.Font.SourceSans;k.Parent=b;local l=Instance.new("LocalScript")local m=Instance.new("Frame")m.Name="DefaultNotif"m.ZIndex=5;m.AnchorPoint=Vector2.new(1,1)m.Visible=false;m.Size=UDim2.new(0,291,0,56)m.Position=UDim2.new(1,0,0.9999999,0)m.BackgroundColor3=Color3.fromRGB(48,48,48)m.Parent=a;local n=Instance.new("UICorner")n.Name="UiCorner"n.Parent=m;local o=Instance.new("UIStroke")o.Name="Dropshadow"o.ApplyStrokeMode=Enum.ApplyStrokeMode.Border;o.Transparency=0.8;o.Thickness=2;o.Color=Color3.fromRGB(20,20,20)o.Parent=m;local p=Instance.new("TextLabel")p.Name="Title"p.Size=UDim2.new(0,291,0,19)p.BackgroundTransparency=1;p.BorderSizePixel=0;p.BackgroundColor3=Color3.fromRGB(255,255,255)p.FontSize=Enum.FontSize.Size14;p.TextSize=14;p.TextColor3=Color3.fromRGB(255,255,255)p.Font=Enum.Font.SourceSans;p.Parent=m;local q=Instance.new("TextLabel")q.Name="Paragraph"q.Size=UDim2.new(0,291,0,37)q.BackgroundTransparency=1;q.Position=UDim2.new(0,0,0.3392857,0)q.BackgroundColor3=Color3.fromRGB(255,255,255)q.FontSize=Enum.FontSize.Size14;q.TextSize=14;q.TextColor3=Color3.fromRGB(255,255,255)q.Text=""q.TextYAlignment=Enum.TextYAlignment.Top;q.TextWrapped=true;q.Font=Enum.Font.SourceSans;q.TextWrap=true;q.TextXAlignment=Enum.TextXAlignment.Left;q.Parent=m;local r=Instance.new("UIPadding")r.PaddingLeft=UDim.new(0,10)r.PaddingRight=UDim.new(0,5)r.Parent=q;if syn then syn.protect_gui(a)end;a.Parent=game:GetService("Players").LocalPlayer.PlayerGui;return a end;local s={}notificationHolder=createBaseNotifications()notifAmount=0;removedPos=nil;function s:CreatePromptNotif(t)t=t or{}t.TweenSpeed=t.TweenSpeed or 1;t.TweenInSpeed=t.TweenInSpeed or t.TweenSpeed;t.TweenOutSpeed=t.TweenOutSpeed or t.TweenSpeed;t.TweenVerticalSpeed=t.TweenVerticalSpeed or t.TweenSpeed;t.Title=t.Title or"Title"t.Text=t.Text or"Text"t.TrueText=t.TrueText or"Yes"t.FalseText=t.FalseText or"No"t.Duration=t.Duration or 5;t.Callback=t.Callback or function()warn("No callback for notif")end;notifAmount=notifAmount+1;local u=notifAmount;local v=notifAmount;local w=true;local x=notificationHolder.ToggleNotif:Clone()local y=false;x.Parent=notificationHolder;x.Visible=true;x.Position=UDim2.new(1,300,1,-5)x.Transparency=0.05;x.True.Text=t.TrueText;x.False.Text=t.FalseText;task.spawn(function()task.wait(t.Duration+t.TweenInSpeed)w=false end)x.True.MouseButton1Click:Connect(function()w=false;y=true;notifAmount=notifAmount-1;removedPos=x.Position.Y.Offset;pcall(t.Callback,true)end)x.False.MouseButton1Click:Connect(function()w=false;y=true;notifAmount=notifAmount-1;removedPos=x.Position.Y.Offset;pcall(t.Callback,false)end)x.Paragraph.Text=t.Text;x.Title.Text=t.Title;x:TweenPosition(UDim2.new(1,-5,1,-5),Enum.EasingDirection.Out,Enum.EasingStyle.Quint,t.TweenInSpeed)task.spawn(function()local z=x.Position;while w and task.wait()do local A=x.Position;if notifAmount>u then x:TweenPosition(UDim2.new(1,-5,1,z.Y.Offset-65*(notifAmount-v)),Enum.EasingDirection.Out,Enum.EasingStyle.Quint,t.TweenVerticalSpeed,true)u=u+1 end;if notifAmount<u then if removedPos>A.Y.Offset then x:TweenPosition(UDim2.new(1,-5,1,z.Y.Offset-65*(notifAmount-v)),Enum.EasingDirection.Out,Enum.EasingStyle.Quint,t.TweenVerticalSpeed,true)else v=v-1 end;u=u-1 end end;local A=x.Position;if y==false then notifAmount=notifAmount-1;removedPos=x.Position.Y.Offset end;x:TweenPosition(UDim2.new(1,300,1,A.Y.Offset),Enum.EasingDirection.Out,Enum.EasingStyle.Quint,t.TweenOutSpeed,true)task.wait(t.TweenOutSpeed)x:Destroy()end)end;function s:CreateDefaultNotif(t)t=t or{}t.TweenSpeed=t.TweenSpeed or 1;t.TweenInSpeed=t.TweenInSpeed or t.TweenSpeed;t.TweenOutSpeed=t.TweenOutSpeed or t.TweenSpeed;t.TweenVerticalSpeed=t.TweenVerticalSpeed or t.TweenSpeed;t.Title=t.Title or"Title"t.Text=t.Text or"Text"t.Duration=t.Duration or 5;notifAmount=notifAmount+1;local u=notifAmount;local v=notifAmount;local y=false;local w=true;local x=notificationHolder.DefaultNotif:Clone()x.Parent=notificationHolder;x.Visible=true;x.Position=UDim2.new(1,300,1,-5)x.Transparency=0.05;x.InputBegan:Connect(function(B)if B.UserInputType==Enum.UserInputType.MouseButton1 then task.spawn(function()local C=TweenInfo.new(0.5,Enum.EasingStyle.Linear,Enum.EasingDirection.Out,0,false,0)game:GetService("TweenService"):Create(x,C,{Transparency=0.8}):Play()end)w=false;y=true;notifAmount=notifAmount-1;removedPos=x.Position.Y.Offset end end)task.spawn(function()task.wait(t.Duration+t.TweenInSpeed)w=false end)x.Paragraph.Text=t.Text;x.Title.Text=t.Title;x:TweenPosition(UDim2.new(1,-5,1,-5),Enum.EasingDirection.Out,Enum.EasingStyle.Quint,t.TweenInSpeed)task.spawn(function()local z=x.Position;while w and task.wait()do local A=x.Position;if notifAmount>u then x:TweenPosition(UDim2.new(1,-5,1,z.Y.Offset-65*(notifAmount-v)),Enum.EasingDirection.Out,Enum.EasingStyle.Quint,t.TweenVerticalSpeed,true)u=u+1 end;if notifAmount<u then if removedPos>A.Y.Offset then x:TweenPosition(UDim2.new(1,-5,1,z.Y.Offset-65*(notifAmount-v)),Enum.EasingDirection.Out,Enum.EasingStyle.Quint,t.TweenVerticalSpeed,true)else v=v-1 end;u=u-1 end end;local A=x.Position;if y==false then notifAmount=notifAmount-1;removedPos=x.Position.Y.Offset end;x:TweenPosition(UDim2.new(1,300,1,A.Y.Offset),Enum.EasingDirection.Out,Enum.EasingStyle.Quint,t.TweenOutSpeed,true)task.wait(t.TweenOutSpeed)x:Destroy()end)end;return s
